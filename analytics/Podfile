# AnalyticsExample

use_frameworks!
platform :ios, '9.0'

target 'AnalyticsExample' do
  pod 'Firebase/Analytics', '~> 6.0'
  pod 'FSAdSDK/Banner', :git => 'https://github.com/freestarcapital/Freestar-Mobile-iOS-SDK.git', :branch => 'analytics'  
  pod 'Google-Mobile-Ads-SDK', '~> 7.50.0'
  
  target 'AnalyticsExampleTests' do
    inherit! :search_paths
  end
end

target 'AnalyticsExampleSwift' do
  pod 'Firebase/Analytics', '~> 6.0'
  pod 'FSAdSDK/Banner', :git => 'https://github.com/freestarcapital/Freestar-Mobile-iOS-SDK.git', :branch => 'analytics'
  pod 'Google-Mobile-Ads-SDK', '~> 7.50.0'
end
